#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main(){
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin>>t;
	while(t--){
		string a;
		cin>>a;
		int n=a.size(),ok=0;
		for(int i=0;i<n;i++){
			if(a[i]!='0'&&a[i]!='6'&&a[i]!='8'){
				ok=1;
				break;
			}
		}
		if(ok==0) cout<<"YES"<<endl;
		else cout<<"NO"<<endl;
	}
}
