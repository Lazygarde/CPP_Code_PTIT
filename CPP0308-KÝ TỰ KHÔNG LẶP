#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main(){
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin>>t;
	while(t--){
		string a;
		cin>>a;
		int b[26]={},n=a.size();
		for(int i=0;i<n;i++){
			b[a[i]-'A']++;
		}
		for(int i=0;i<26;i++) if(b[i]==1) cout<<(char)(i+'A');
		cout<<endl;
	}
}
