#include <bits/stdc++.h>
#define a() a
using namespace std;
class NhanVien{
	public:
	string manv,ten,gt,ns,dc,ms,nk;
	friend istream &operator>>(istream &input,NhanVien &a){
		a.manv="00001";
		getline(cin,a.ten);
		cin>>a.gt>>a.ns;
		cin.ignore();
		getline(cin,a.dc);
		cin>>a.ms>>a.nk;
		return input;
	}
	friend ostream &operator<<(ostream &output,NhanVien a){
		cout<<a.manv<<" "<<a.ten<<" "<<a.gt<<" "<<a.ns<<" "<<a.dc<<" "<<a.ms<<" "<<a.nk<<endl;
		return output;
	}
};
int main(){
    NhanVien a();
    cin >> a;
    cout << a;
    return 0;
}
