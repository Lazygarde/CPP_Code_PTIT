#include <bits/stdc++.h>
using namespace std;
struct ThiSinh{
	string ten,ns;
	float s1,s2,s3;
};
void nhap(struct ThiSinh &a){
	getline(cin,a.ten);
	cin>>a.ns>>a.s1>>a.s2>>a.s3;
}
void in(struct ThiSinh a){
	cout<<a.ten<<" "<<a.ns<<" ";
	printf("%.1f",a.s1+a.s2+a.s3);
}
int main(){
	struct ThiSinh A;
    nhap(A);
    in(A);
    return 0;
}
