#include <bits/stdc++.h>
using namespace std;
int main(){
	int t;
	cin>>t;
	while(t--){
		int x,n=0,c=0,l=0;
		while(scanf("%d",&x)!=-1){
			if(x%2==0) c++;
			else l++;
			n++;
			char k=getchar();
			if(k=='\n') break;
		}
		if((n%2==0&&c>l)||(n%2==1&&c<l)) cout<<"YES\n";
		else cout<<"NO\n";
	}
}
