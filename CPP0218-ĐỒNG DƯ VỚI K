#include<bits/stdc++.h>
using namespace std;
int main(){
	int t;
	cin>>t;
	while(t--){
		int n,ss=0;
		cin>>n;
		int a[n];
		for(int i=0;i<n;i++) cin>>a[i];
		sort(a,a+n);
		int s=a[1]-a[0];
		for(int i=2;i<n;i++){
			int k=a[i]-a[i-1];
			s=__gcd(s,k);
		}
		for(int i=1;i<=sqrt(s);i++){
			if(s%i==0){
				if(i!=s/i) ss+=2;
				else ss++;
			}
		}
		cout<<ss<<endl;
	}
}
