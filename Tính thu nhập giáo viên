#include <bits/stdc++.h>
using namespace std;
int main(){
    string id,name,tmp="",tmp2="";
    long long l1,l2,s=1;
    getline(cin,id);
    getline(cin,name);
    cin>>l1;
    tmp.push_back(id[0]);
    tmp.push_back(id[1]);
    tmp2.push_back(id[2]);
    tmp2.push_back(id[3]);
    s=stoll(tmp2);
    if(tmp=="HT") l2=2000000;
    else if(tmp=="HP") l2=900000;
    else l2 = 500000;
    cout<<id<<" "<<name<<" "<<s<<" "<<l2<<" "<<l1*s+l2;
}
