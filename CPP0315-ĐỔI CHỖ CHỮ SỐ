#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main(){
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin>>t;
	while(t--){
		string a;
		cin>>a;
		int n=a.size(),ok=0;
		for(int i=0;i<n-1;i++){
			if(a[i]>a[i+1]){
				ok=1;
				swap(a[i],a[i+1]);
				break;
			}
		}
		if(ok==0) cout<<-1<<endl;
		else cout<<a<<endl;
	}
}
