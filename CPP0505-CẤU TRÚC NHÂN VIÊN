#include <bits/stdc++.h>
using namespace std;
struct NhanVien{
	string ten,gt,ns,dc,ms,nk;
};
void nhap(struct NhanVien &a){
	getline(cin,a.ten);
	cin>>a.gt>>a.ns;
	cin.ignore();
	getline(cin,a.dc);
	cin>>a.ms>>a.nk;
}
void in(struct NhanVien a){
	cout<<"00001 "<<a.ten<<" "<<" "<<a.gt<<" "<<a.ns<<" "<<a.dc<<" "<<a.ms<<" "<<a.nk;
}
int main(){
	struct NhanVien a;
    nhap(a);
    in(a);
    return 0;
}
