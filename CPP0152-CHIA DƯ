#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main(){
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin>>t;
	while(t--){
		int a,m,ok=0;
		cin>>a>>m;
		for(int i=0;i<m;i++){
			if((a*i-1)%m==0){
				ok=1;
				cout<<i<<endl;
				break;
			}
		}
		if(ok==0) cout<<-1<<endl;
	}
}
